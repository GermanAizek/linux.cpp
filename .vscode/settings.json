{
    "clangd.path": "/usr/bin/clangd",
    "clangd.arguments": [
      "--compile-commands-dir=${workspaceFolder}/build",
      "--query-driver=aarch64-linux-gnu-gcc",
      "--all-scopes-completion=false",
      "--background-index",
      "--background-index-priority=normal",
      "--clang-tidy",
      "--completion-style=detailed",
      "--experimental-modules-support",
      "--function-arg-placeholders=1",
      "--header-insertion=iwyu",
      "--header-insertion-decorators",
      "--limit-references=0",
      "--limit-results=0",
      "--rename-file-limit=0",
      "--log=verbose",
      "--pretty",
      "-j=4",
      "--pch-storage=disk",
      "--enable-config",
    ],
    "favorites.resources": [
        {
            "filePath": "drivers/misc/ethosn",
            "group": "Default"
        },
        {
            "filePath": ".clang-uml",
            "group": "Default"
        },
        {
            "filePath": "build/puml",
            "group": "Default"
        },
        {
            "filePath": "drivers/misc/ethosn/juno-r2-ethosn-smmu.dts",
            "group": "Default"
        }
    ],
    "favorites.sortOrder": "ASC",
}