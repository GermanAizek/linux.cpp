{
    "clangd.path": "/usr/bin/clangd",
    "clangd.arguments": [
      "--compile-commands-dir=${workspaceFolder}/build",
      "--log=verbose",
      "--pretty",
      "--all-scopes-completion",
      "--completion-style=bundled",
      "--cross-file-rename",
      "--header-insertion=iwyu",
      "--header-insertion-decorators",
      "--background-index",
      "--clang-tidy",
      "-j=4",
      "--pch-storage=disk",
      "--function-arg-placeholders=false",
      "--query-driver=aarch64-linux-gnu-gcc",
    ],
    "favorites.resources": [
        {
            "filePath": "drivers/misc/ethosn",
            "group": "Default"
        },
        {
            "filePath": ".clang-uml",
            "group": "Default"
        },
        {
            "filePath": "build/puml",
            "group": "Default"
        }
    ],
    "favorites.sortOrder": "ASC",
}